{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ],
    "transform-decorators",
    ["lodash", { "id": ["lodash", "recompose"] }],
    "ramda",
    "graphql-tag",
    "polished"
  ],
  "env": {
    "test": {
      "presets": [["env", { "modules": "commonjs" }], "next/babel"]
    }
  }
}
